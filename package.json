{
    "name": "mern-example",
    "version": "1.0.0",
    "description": "A mern stack example",
    "main": "public/index.html",
    "scripts": {
        "start": "concurrently \"node server/server.js\" \"open http://localhost:3000\"",
        "startmon": "concurrently \"nodemon -w server -e js,graphql server/server.js\" \"open http://localhost:3000\"",
        "compile": "babel src --out-dir public",
        "watch": "babel src --out-dir public --watch --verbose",
        "test": "echo \"Error: no test specified\" && exit 1"
    },
    "author": "Lauren Clark",
    "license": "ISC",
    "dependencies": {
        "apollo-server-express": "^2.3.1",
        "express": "^4.16.4",
        "graphql": "^0.13.2",
        "nodemon": "^1.18.9"
    },
    "devDependencies": {
        "@babel/cli": "^7.2.3",
        "@babel/core": "^7.2.2",
        "@babel/preset-env": "^7.2.3",
        "@babel/preset-react": "^7.0.0",
        "concurrently": "^5.0.2"
    }
}
